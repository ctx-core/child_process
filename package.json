{
	"name": "@ctx-core/child_process",
	"version": "7.4.177",
	"description": "ctx-core child_process extensions",
	"keywords": [
		"ctx-core",
		"chain"
	],
	"homepage": "https://github.com/ctx-core/child_process#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/child_process/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/child_process.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-child_process@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./child_process__capture",
		"./child_process__pipe",
		"./child_process__pipe_",
		"./child_process__receive",
		"./exec",
		"./spawn_pipe_process",
		"./stdout_stderr__pipe"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^7.1.3",
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}