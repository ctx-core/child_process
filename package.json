		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.107",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
